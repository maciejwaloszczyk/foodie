{
  "kind": "collectionType",
  "collectionName": "dishes",
  "info": {
    "singularName": "dish",
    "pluralName": "dishes",
    "displayName": "Dish"
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "name": {
      "type": "string",
      "required": true
    },
    "description": {
      "type": "blocks"
    },
    "price": {
      "type": "decimal"
    },
    "category": {
      "type": "string"
    },
    "rating": {
      "type": "decimal"
    },
    "restaurant": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::restaurant.restaurant",
      "inversedBy": "dishes"
    },
    "dish_attributes": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::dish-attribute.dish-attribute",
      "mappedBy": "dish"
    },
    "reviews": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::review.review",
      "mappedBy": "dish"
    }
  }
}
