{
  "kind": "collectionType",
  "collectionName": "restaurants",
  "info": {
    "singularName": "restaurant",
    "pluralName": "restaurants",
    "displayName": "Restaurant"
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "name": {
      "type": "string",
      "required": true
    },
    "address": {
      "type": "string"
    },
    "city": {
      "type": "string"
    },
    "latitude": {
      "type": "decimal"
    },
    "longitude": {
      "type": "decimal"
    },
    "avg_rating": {
      "type": "decimal",
      "default": 0
    },
    "dishes": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::dish.dish",
      "mappedBy": "restaurant"
    }
  }
}
